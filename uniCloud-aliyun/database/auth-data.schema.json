// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": true,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"platform": {
			"bsonType": "string",
			"description": "平台"
		},
		"refresh_token": {
			"bsonType": "string",
			"description": "refresh_token值"
		},
		"session_key": {
			"bsonType": "string",
			"description": "session_key值"
		},
		"access_token": {
			"bsonType": "string",
			"description": "access_token值"
		},
		"scope": {
			"bsonType": "string",
			"description": "scope值"
		},
		"session_secret": {
			"bsonType": "string",
			"description": "session_secret值"
		},
		"detail": {
			"bsonType": "object",
			"description": "回调内容"
		},
		"expires_in": {
			"bsonType": "int",
			"description": "过期时间"
		},
		"last_update_date": {
			"bsonType": "timestamp",
			"description": "最后一次修改时间",
			"forceDefaultValue": {
				"$env": "now"
			}
		}
	}
}